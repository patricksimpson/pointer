{
  "name": "pointer",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "dependencies": {
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.1",
    "babel-loader": "^8.1.0",
    "npm-run-all": "^4.1.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "socket.io": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "scripts": {
    "build": "npm run test && npm run lint && webpack --mode production && npm run build:create",
    "asset:copy": "cp -R ./src/static ./build/ && cp ./src/index.html ./build/index.html && cp ./src/brushColors.js ./build/brushColors.js",
    "build:create": "mkdir -p build && npm run asset:copy && cp -R ./dist ./build/",
    "clean": "rm -rf ./build ./dist",
    "lint": "eslint src/**/*.js",
    "start": "run-p server test:watch",
    "server": "webpack-dev-server --mode development",
    "test": "mocha --require ./spec-helper.js --require @babel/register --recursive \"./src/**/*-spec.js\"",
    "test:watch": "npm run test -- --watch"
  },
